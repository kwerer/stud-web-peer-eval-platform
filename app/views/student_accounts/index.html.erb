<p style="color: green"><%= notice %></p>

<h1>Student accounts</h1>

<div id="student_accounts">
  <% @student_accounts.each do |student_account| %>
    <%= render student_account %>
    <p>
      <%= link_to "Show this student account", student_account %>
    </p>
  <% end %>
</div>

<%= link_to "New student account", new_student_account_path %>
